use "tygronenv-1.0.6-jar-with-dependencies.jar" as environment 
	with map = "vh3".

define DUWOAgent as agent {
	use "DUWOagent//DUWOmain" as main module.
	use "DUWOagent//DUWOevents" as event module.
	use "DUWOagent//DUWOinit" as init module.
}

launchpolicy {
	when name=entity launch DUWOAgent.
}
