use predicates as knowledge.
use perceptEvents as module.
use goalEvents as module.
use MunicipalityProtocol as module.
use requestEvents as module.

order=linearall.

module events {
	
	
	% Processing percepts
	if true then perceptEvents.
	
	% Adopting goals
	if true then goalEvents.
	
	% Process requests
	if true then MunicipalityProtocol.
	
	if true then requestEvents.

}
